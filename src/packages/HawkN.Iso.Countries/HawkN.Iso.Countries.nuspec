        <file src="bin\Release\net8.0\HawkN.Iso.Countries.dll" target="lib\net8.0\HawkN.Iso.Countries.dll" />
        <file src="bin\Release\net8.0\HawkN.Iso.Countries.xml" target="lib\net8.0\HawkN.Iso.Countries.xml" />
    </files>
</package>